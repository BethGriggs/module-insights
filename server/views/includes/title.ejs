<!-- TITLE_BEGIN -->
<div id="ibm-leadspace-head" class="ibm-alternate ibm-padding-top-1 ibm-background-green-10">
  <div id="ibm-leadspace-body ibm-padding-bottom-0">
    <div class="ibm-columns ibm-center">
        <h1 class="ibm-h1 ibm-medium ibm-textcolor-default ibm-padding-top-0 ibm-padding-bottom-0">
          Module Insights
          <h3 class="ibm-h5 ibm-textcolor-default ibm-padding-bottom-0">
            Version:
            <%= app_version %>
          </h3>
          <% if (typeof links !== 'undefined') { %>
            <p class="ibm-btn-row ibm-button-link ibm-padding-top-1 ">
              <a target="_blank" rel="noopener noreferrer" href="https://developer.ibm.com/technologies/node-js/" class="ibm-external-link ibm-icon-after link-primary">Node.js@IBM Developer</a>
              <a target="_blank" rel="noopener noreferrer" href="https://www.ibm.com/us-en/marketplace/support-for-runtimes" class="ibm-external-link ibm-icon-after link-primary">IBM Support</a>
              <a target="_blank" rel="noopener noreferrer" href="https://www.cloudnativejs.io/modules-faq.html" class="ibm-external-link ibm-icon-after link-primary">Docs</a>
            </p>
            <% }; %>
                <% if (breadcrumb && breadcrumb.length) { %>
                  <nav role="navigation" class="ibm-textcolor-default ibm-padding-bottom-0" aria-label="Breadcrumb">
                      <ul id="ibm-navigation-trail">
                      <% breadcrumb.forEach((crumb) => {
                      let colour = 'default';
                      %>
                        <li itemscope="" itemtype="//data-vocabulary.org/Breadcrumb">
                          <% if (crumb.link !== '') {
                          let colour = 'green-40';
                          %>
                            <a href="<%= crumb.link %>" itemprop="url">
                              <% }; %>
                                <span itemprop="title" class="ibm-textcolor-<%= colour %>">
                                  <%= crumb.page %>
                                </span>
                                <% if (crumb.link !== '') { %>
                            </a>
                            <% }; %>
                        </li>
                    <% }) %>
                  </ul>
                  </nav>
          <% } %>
        </h1>
    </div>
  </div>
  <div class="version">
    <div class="demo version-section"><a href="https://github.com/CloudNativeJS/module-insights" class="github-corner" aria-label="View source on GitHub">
        <svg width="80" height="80" viewBox="0 0 250 250" style="fill:rgb(16, 50, 54); color:white; position: absolute; top: 0; border: 0; right: 0;" aria-hidden="true">
